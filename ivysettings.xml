<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <settings defaultResolver="chain" />
    <resolvers>
        <ibiblio name="ibiblio" m2compatible="true" checksums="" />
        <ibiblio name="wetator-sh"
                 m2compatible="true"
                 usepoms="true"
                 root="http://www.wetator.org/repo"
                 checkmodified="true"
                 changingPattern="^.+-SNAPSHOT$"/>
        <url name="wetator" checkmodified="true" changingPattern="^.+-SNAPSHOT$">
            <artifact pattern="http://www.wetator.org/repo/[module]/[revision]/[artifact]-[revision].[ext]" />
            <artifact pattern="http://www.wetator.org/repo/[module]/[revision]/[artifact].[ext]" />
        </url>
        <chain name="chain">
            <resolver ref="wetator-sh" />
            <resolver ref="wetator" />
            <resolver ref="ibiblio" />
        </chain>
    </resolvers>
</ivysettings>
