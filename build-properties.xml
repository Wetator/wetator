<?xml version="1.0" encoding="UTF-8"?>
<build-properties>

  <application>
    <name>Wetator</name>
    <version>0_9_13_beta4</version>
    <pubversion>0.9.13-SNAPSHOT</pubversion>
    <jar-basename>wetator</jar-basename>
    <vendor>wetator.org</vendor>
  </application>

  <dir>
    <src>src</src>
    <test>test</test>
    <test-src>${dir.test}/java</test-src>
    <samples>samples</samples>
    <lib>lib</lib>
    <lib-src>lib_src</lib-src>
    <ant-lib>ant_lib</ant-lib>
    <build>build</build>
    <xsl>xsl</xsl>
    <webpages>webpages</webpages>
    <deploy>deploy</deploy>
  </dir>

  <ivy>
    <retrieve>
        <pattern>/[artifact]-[revision](-[classifier]).[ext]</pattern>
    </retrieve>
  </ivy>

  <javac>
    <target>1.6</target>
  </javac>

  <!-- display number (used for xvfb in CI only) -->
  <display>1</display>

</build-properties>
