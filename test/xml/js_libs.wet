    <command><d:open-url><d:url>javascript_jquery.html</d:url></d:open-url></command>
    <command><d:assert-title><d:title>Wetator / JavaScript jQuery</d:title></d:assert-title></command>
    <command><d:assert-content><d:content>jQuery version: 1.2.1</d:content></d:assert-content></command>
    <comment>boxover test</comment>
    <command><d:open-url><d:url>javascript_boxover.html</d:url></d:open-url></command>
    <command disabled="true"><d:assert-content><d:content>Wetator / JavaScript boxover</d:content></d:assert-content></command>
