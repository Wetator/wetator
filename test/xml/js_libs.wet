    <command><d:assert-content><d:content>Some sort of explanation would go here</d:content></d:assert-content></command>
    <command><d:mouse-over><d:wpath>tooltip with a delay added</d:wpath></d:mouse-over></command>
    <command><d:assert-content><d:content>Delayed tooltip</d:content></d:assert-content></command>
    <command><d:mouse-over><d:wpath>Move over this for a delay and a fade</d:wpath></d:mouse-over></command>
    <command><d:assert-content><d:content>Tooltip with fade &amp; delay </d:content></d:assert-content></command>
