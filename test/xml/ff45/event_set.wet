<?xml version='1.0' encoding='UTF-8'?>
<test-case
    xmlns='http://www.wetator.org/xsd/test-case'
    xmlns:d='http://www.wetator.org/xsd/default-command-set'
    xmlns:tst='http://www.wetator.org/xsd/test-command-set'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://www.wetator.org/xsd/test-case test-case-1.0.0.xsd http://www.wetator.org/xsd/default-command-set default-command-set-1.0.0.xsd http://www.wetator.org/xsd/test-command-set test-command-set-1.0.0.xsd'
    version='1.0.0'>
    <command><d:open-url><d:url>event_handler.html</d:url></d:open-url></command>
    <command><d:assert-title><d:title>Wetator / Event Handler</d:title></d:assert-title></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment>Set input text</comment>
    <command><d:set><d:wpath>InputText</d:wpath><d:value>te</d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [inputText] [1] focus [inputText] [-] mouseup [inputText] [1] click [inputText] [1] keydown [inputText] [84] keypress [inputText] [116] keyup [inputText] [84] keydown [inputText] [69] keypress [inputText] [101] keyup [inputText] [69] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment>Set input password</comment>
    <command><d:set><d:wpath>InputPassword</d:wpath><d:value>te</d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [inputPassword] [1] focus [inputPassword] [-] mouseup [inputPassword] [1] click [inputPassword] [1] keydown [inputPassword] [84] keypress [inputPassword] [116] keyup [inputPassword] [84] keydown [inputPassword] [69] keypress [inputPassword] [101] keyup [inputPassword] [69] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment>Set text area</comment>
    <command><d:set><d:wpath>Textarea</d:wpath><d:value>te</d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [textarea] [1] focus [textarea] [-] mouseup [textarea] [1] click [textarea] [1] keydown [textarea] [84] keypress [textarea] [116] keyup [textarea] [84] keydown [textarea] [69] keypress [textarea] [101] keyup [textarea] [69] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment></comment>
    <comment>Reset input text</comment>
    <command><d:set><d:wpath>InputText</d:wpath><d:value></d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [inputText] [1] focus [inputText] [-] mouseup [inputText] [1] click [inputText] [1] keydown [inputText] [46] keypress [inputText] [46] keyup [inputText] [46] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment>Reset input password</comment>
    <command><d:set><d:wpath>InputPassword</d:wpath><d:value></d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [inputPassword] [1] focus [inputPassword] [-] mouseup [inputPassword] [1] click [inputPassword] [1] keydown [inputPassword] [46] keypress [inputPassword] [46] keyup [inputPassword] [46] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <comment>Reset text area</comment>
    <command><d:set><d:wpath>Textarea</d:wpath><d:value></d:value></d:set></command>
    <command><d:assert-content><d:content>Handler mousedown [textarea] [1] focus [textarea] [-] mouseup [textarea] [1] click [textarea] [1] keydown [textarea] [46] keypress [textarea] [46] keyup [textarea] [46] OK</d:content></d:assert-content></command>
</test-case>