<?xml version='1.0' encoding='UTF-8'?>
<test-case
    xmlns='http://www.wetator.org/xsd/test-case'
    xmlns:d='http://www.wetator.org/xsd/default-command-set'
    xmlns:tst='http://www.wetator.org/xsd/test-command-set'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://www.wetator.org/xsd/test-case test-case-1.0.0.xsd http://www.wetator.org/xsd/default-command-set default-command-set-1.0.0.xsd http://www.wetator.org/xsd/test-command-set test-command-set-1.0.0.xsd'
    version='1.0.0'>
    <command><d:open-url><d:url>event_handler.html</d:url></d:open-url></command>
    <command><d:assert-title><d:title>Wetator / Event Handler</d:title></d:assert-title></command>
    <comment>Select for enabled elements</comment>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><d:select><d:wpath>SingleSelect, option1</d:wpath></d:select></command>
    <command><d:select><d:wpath>SingleSelect, option1</d:wpath></d:select></command>
    <command disabled="true"><d:assert-content><d:content>Handler mousedown [singleSelect] [1] focus [singleSelect] [-] mouseup [singleSelect] [1] change [singleSelect] [-] click [singleSelect] [1] mousedown [singleSelect] [1] mouseup [singleSelect] [1] click [singleSelect] [1] OK</d:content></d:assert-content></command>
    <command><d:assert-content><d:content>Handler mousedown [singleSelect] [1] mouseup [singleSelect] [1] focus [singleSelect] [-] change [singleSelect] [-] click [singleSelect] [-] click [singleSelect_o_value1] [-] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><d:select><d:wpath>MultipleSelect, option1</d:wpath></d:select></command>
    <command><d:select><d:wpath>MultipleSelect, option1</d:wpath></d:select></command>
    <command disabled="true"><d:assert-content><d:content>Handler mousedown [multipleSelect] [1] focus [multipleSelect] [-] mouseup [multipleSelect] [1] change [multipleSelect] [-] click [multipleSelect] [1] mousedown [multipleSelect] [1] mouseup [multipleSelect] [1] click [multipleSelect] [1] OK</d:content></d:assert-content></command>
    <command><d:assert-content><d:content>Handler mousedown [multipleSelect] [1] mouseup [multipleSelect] [1] focus [multipleSelect] [-] change [multipleSelect] [-] click [multipleSelect] [-] click [multipleSelect_o_value1] [-] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><d:select><d:wpath>Checkbox</d:wpath></d:select></command>
    <command><d:select><d:wpath>Checkbox</d:wpath></d:select></command>
    <command disabled="true"><d:assert-content><d:content>Handler mousedown [checkbox] [1] focus [checkbox] [-] mouseup [checkbox] [1] click [checkbox] [1] change [checkbox] [-] mousedown [checkbox] [1] mouseup [checkbox] [1] click [checkbox] [1] change [checkbox] [-] OK</d:content></d:assert-content></command>
    <command><d:assert-content><d:content>Handler mousedown [checkbox] [1] focus [checkbox] [-] mouseup [checkbox] [1] click [checkbox] [-] OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><d:select><d:wpath>RadioGroup, Radio1</d:wpath></d:select></command>
    <command><d:select><d:wpath>RadioGroup, Radio1</d:wpath></d:select></command>
    <command disabled="true"><d:assert-content><d:content>Handler mousedown [radioGroup] [1] focus [radioGroup] [-] mouseup [radioGroup] [1] click [radioGroup] [1] change [radioGroup] [-] mousedown [radioGroup] [1] mouseup [radioGroup] [1] click [radioGroup] [1] OK</d:content></d:assert-content></command>
    <command><d:assert-content><d:content>Handler mousedown [radioGroup] [1] focus [radioGroup] [-] mouseup [radioGroup] [1] click [radioGroup] [-] OK</d:content></d:assert-content></command>
    <comment>Select for disabled elements</comment>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><tst:assert-fail><tst:command-and-message>Select, The html element [HtmlOption 'option1' (id='singleSelectDisabled_o_value1') part of [HtmlSelect (id='singleSelectDisabled') (name='singleSelectDisabled')]] is disabled. Setting a value is not possible.</tst:command-and-message><tst:parameter1>singleSelectDisabled_o_value1</tst:parameter1></tst:assert-fail></command>
    <command><d:assert-content><d:content>Handler OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><tst:assert-fail><tst:command-and-message>Select, The html element [HtmlOption 'option1' (id='multipleSelectDisabled_o_value1') part of [HtmlSelect (id='multipleSelectDisabled') (name='multipleSelectDisabled')]] is disabled. Setting a value is not possible.</tst:command-and-message><tst:parameter1>multipleSelectDisabled_o_value1</tst:parameter1></tst:assert-fail></command>
    <command><d:assert-content><d:content>Handler OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><tst:assert-fail><tst:command-and-message>Select, The html element [HtmlCheckBoxInput (id='checkboxDisabled') (name='checkboxDisabled')] is disabled. Setting a value is not possible.</tst:command-and-message><tst:parameter1>checkboxDisabled</tst:parameter1></tst:assert-fail></command>
    <command><d:assert-content><d:content>Handler OK</d:content></d:assert-content></command>
    <command><d:click-on><d:wpath>Clear</d:wpath></d:click-on></command>
    <command><tst:assert-fail><tst:command-and-message>Select, The html element [HtmlRadioButtonInput 'r_value1' (id='radio1Disabled') (name='radioGroup')] is disabled. Setting a value is not possible.</tst:command-and-message><tst:parameter1>radio1Disabled</tst:parameter1></tst:assert-fail></command>
    <command><d:assert-content><d:content>Handler OK</d:content></d:assert-content></command>
</test-case>